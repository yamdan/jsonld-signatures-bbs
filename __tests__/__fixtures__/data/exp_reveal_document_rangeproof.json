{
  "@context": [
    "https://www.w3.org/2018/credentials/v1",
    "https://www.zkp-ld.org/bbs-termwise-2021.jsonld",
    "https://schema.org"
  ],
  "type": ["VerifiableCredential"],
  "@explicit": true,
  "issuer": {},
